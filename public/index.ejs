<script type="text/x-handlebars-template" id="activity-template">

    {{#each response}}
    <div class="media stream well" style="margin-left: 10px;margin-right: 10px;">
        <a href="#" class="media-avatar medium pull-left">
            <img src="images/samples/phone.png">
        </a>
        <div class="media-body">
            <div class="stream-headline">
                <h5 class="stream-author">
                    {{WRK_TYP_DSCR}}
                    <small>{{formatDate  WRK_UPDT_TS "long"}}   {{#if WRK_LOC}}  {{WRK_LOC}} {{else}} New Jersey  {{/if}} </small>
                </h5>
                <div class="stream-text">
                    Address change request raised on the contract effective 11/01/2013.Address verification document has been requested for servicing.
                    Service request 6110 created.
                </div>
            </div><!--/.stream-headline-->
        </div>
    </div><!--/.media .stream-->
    {{/each}}
</script>

