<script type="text/x-handlebars-template" id="tmplHEALTH_CARE_PROVIDERS">
    <div class="module  clearfix">
        <div class="module-head">
            <h3>Health Care Providers</h3>
        </div>
        <div class="module-body clearfixx">
            {{#each Providers}}
            <div class="media stream well" style="margin-left: 10px;margin-right: 10px;">
                <a href="#" class="media-avatar medium pull-left">
                    <img src="/images/custom/hp.png">
                </a>
                <div class="media-body">
                    <div class="stream-headline">
                        <h5 class="stream-author">
                            {{[Provider Name]}}
                            <small>{{[Type of Provider]}} </small>
                        </h5>
                        <div class="stream-text">
                           <div class="row">
                               <div class="span3"><span style="font-weight: bold" >Phone Number:</span> {{[Phone Number]}} X {{Ext}}</div>
                               <div class="span3"><span style="font-weight: bold" >Email :</span> {{Email}}</div>
                           </div>
                            <div class="row">
                                <div class="span6"><span style="font-weight: bold" >Comments:</span> {{Comments}}</div>
                            </div>

                        </div>
                    </div><!--/.stream-headline-->
                </div>
            </div><!--/.media .stream-->
            {{/each}}
            <div class="clearfix"></div>
        </div>
    </div>

</script>

